<site name="Sportsnet" homepage="http://www.sportsnet.ca/">
	<example>http://www.sportsnet.ca/soccer/west-ham-2-hull-2/</example>

	<tags><tag>.ca</tag><tag>sports</tag></tags>

	<host>sportsnet.ca</host>
	<scrape>
		<extract>/vid(?:eoId)?=(?'id'\d+)/</extract>
		<extract>/param name="@videoPlayer" value="(?'id'\d+)"/</extract>
	</scrape>
	<iframe src="https://images.rogersdigitalmedia.com/video_service.php?videoId={@id}&amp;playerKey=AQ~~,AAAAAGWRwLc~,cRCmKE8Utf7OFWP38XQcokFZ80fR-u_y&amp;autoStart=false&amp;width=100%25&amp;height=100%25"/>
</site>